{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@pages/*": [
                "src/pages/*"
            ],
            "@types": [
                "types"
            ],
            "@types/*": [
                "src/types/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@src/*": [
                "src/*"
            ],
        }
    }
}